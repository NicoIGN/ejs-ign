{
  "env": {
      "browser": true,
      "es2021": true,
      "amd": true,
      "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
      "require": true,
      "module": true,
      "commonjs": true,
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "global-require": "off",
    "no-inner-declarations": "off",
    "no-param-reassign": ["error", { "props": false }]
  },
  "settings": {
  }
}
